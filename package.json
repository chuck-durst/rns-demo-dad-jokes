{
  "name": "react-next-starter",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "dev": "NODE_ENV=development node server/",
    "build": "next build ./client",
    "start": "NODE_ENV=production node server/",
    "test": "NODE_ENV=test jest --silent ./__tests__",
    "preprod": "now --public",
    "pm2:start": "NODE_ENV=production pm2 start ./config/ecosystem.config.js",
    "pm2:stop": "pm2 kill",
    "now": "now --token $NOW_TOKEN --npm",
    "now:rm": "now rm react-next-starter --yes --token $NOW_TOKEN",
    "now:alias": "now alias --token $NOW_TOKEN $NOW_URL preprod.chefsquare.com",
    "analyze": "cross-env ANALYZE=1 next build ./client"

  },
  "engines": {
    "node": "8.9.0"
  },
  "now": {
    "alias": "preprod.chefsquare.com",
    "env": {
      "HTPASSWD_USER": "@htpasswd-user",
      "HTPASSWD_PASSWORD": "@htpasswd-password",
      "BASE_URL": "preprod.chefsquare.com"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "^0.1.2",
    "autoprefixer": "^8.4.1",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "i18n": "^0.8.3",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-express-middleware": "^1.1.1",
    "i18next-node-fs-backend": "^1.0.0",
    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.3",
    "next": "^6.0.3",
    "next-page-transitions": "^1.0.0-alpha.3",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "node-sass": "^4.9.0",
    "nprogress": "^0.2.0",
    "postcss-loader": "^2.1.4",
    "postcss-sass": "^0.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-i18n": "0.0.6",
    "react-i18next": "^7.6.1",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.21.0",
    "url-join": "^4.0.0",
    "url-loader": "^1.0.1",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "basic-auth": "^2.0.0",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.4.0",
    "request-promise": "^4.2.2"
  }
